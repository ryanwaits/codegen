{
  "name": "@stacks/codegen",
  "version": "0.1.0",
  "description": "CLI tool for generating type-safe Stacks contract interfaces",
  "type": "module",
  "bin": {
    "codegen": "./dist/cli.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.cjs"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "stacks",
    "clarity",
    "blockchain",
    "smart-contracts",
    "cli",
    "codegen"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@antfu/ni": "^24.4.0",
    "@hirosystems/clarinet-sdk": "^3.0.2",
    "@stacks/connect": "^8.1.9",
    "@stacks/transactions": "7.0.6",
    "@tanstack/react-query": "^5.77.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "esbuild": "^0.19.0",
    "execa": "^9.5.3",
    "got": "^13.0.0",
    "ora": "^7.0.1",
    "prettier": "^3.1.0",
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^19.1.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18"
  }
}
